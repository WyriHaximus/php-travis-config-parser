{
    "name": "wyrihaximus/travis-config-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/yaml": "^3.2"
    },
    "require-dev": {
        "jakub-onderka/php-console-highlighter": "^0.3.2",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\Travis\\ConfigParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Tests\\Travis\\ConfigParser\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
